{
	"Table": {
		"$type": "table",
		"style": {
			"border": "3px solid black"
		},
		"children": [
			{
				"$type": "caption",
				"children": {
					"$type": "$val",
					"val": "caption"
				}
			},
			{
				"$type": "$for",
				"for": "index",
				"in": {
					"$type": "$val",
					"val": "data"
				},
				"do": {
					"$type": "TableRow",
					"data": {
						"$type": "$index",
						"index": {
							"$type": "$val",
							"val": "index"
						},
						"in": {
							"$type": "$val",
							"val": "data"
						}
					}
				}
			}
		]
	},
	"TableRow": {
		"$type": "tr",
		"style": {
			"background": "#999"
		},
		"children": {
			"$type": "$for",
			"for": "index",
			"in": {
				"$type": "$val",
				"val": "data"
			},
			"do": {
				"$type": "TableCell",
				"children": {
					"$type": "$index",
					"index": {
						"$type": "$val",
						"val": "index"
					},
					"in": {
						"$type": "$val",
						"val": "data"
					}
				}
			}
		}
	},
	"TableCell": {
		"$type": "td",
		"style": {
			"border": "1px solid black"
		},
		"children": {
			"$type": "$val",
			"val": "children"
		}
	},
	"AnotherTable": {
		"$type": "Table"
	},
	"Box": {
		"$type": "div",
		"style": {
			"border": "3px solid black",
			"float": "left",
			"padding": "5",
			"margin": "5",
			"position": {
				"$type": "$index",
				"index": "position",
				"in": {
					"$type": "$val",
					"val": "style"
				}
			},
			"top": {
				"$type": "$index",
				"index": "top",
				"in": {
					"$type": "$val",
					"val": "style"
				}
			},
			"left": {
				"$type": "$index",
				"index": "left",
				"in": {
					"$type": "$val",
					"val": "style"
				}
			}
		},
		"children": [
			{
				"$type": "div",
				"children": {
					"$type": "$val",
					"val": "name"
				}
			},
			{
				"$type": "$val",
				"val": "children"
			}
		]
	},
	"Boxes": {
		"$type": "div",
		"children": [
			{
				"$type": "Box",
				"name": "My Box",
				"children": [
					{
						"$type": "Box",
						"name": "My Inner Box",
						"children": {
							"$type": "Box",
							"name": "My Inner Inner Box"
						}
					},
					{
						"$type": "Box",
						"name": "My Second Inner Box"
					}
				]
			},
			{
				"$type": "div",
				"style": {
					"clear": "both"
				}
			}
		]
	},
	"Boxes2": {
		"$type": "Box",
		"name": "NOT NAME",
		"children": [
			{
				"$type": "Box",
				"name": {
					"$type": "$val",
					"val": "name"
				}
			}
		]
	},
	"Figure": {
		"$type": "div",
		"children": [
			{
				"$type": "$val",
				"val": "title"
			},
			{
				"$type": "br"
			},
			{
				"$type": "img",
				"src": {
					"$type": "$val",
					"val": "src"
				},
				"alt": {
					"$type": "$val",
					"val": "title"
				},
				"title": {
					"$type": "$val",
					"val": "title"
				}
			}
		]
	},
	"Container": {
		"$type": "div",
		"style": {
			"width": "100%",
			"height": "600",
			"position": "relative"
		},
		"children": {
			"$type": "$val",
			"val": "children"
		}
	}
}
