{
	"Table": {
		"root": "table",
		"style": {
			"border": "3px solid black"
		},
		"children": [
			{
				"root": "caption",
				"children": {
					"root": "$val",
					"val": "caption"
				}
			},
			{
				"root": "$for",
				"for": "index",
				"in": {
					"root": "$val",
					"val": "data"
				},
				"do": {
					"root": "TableRow",
					"data": {
						"root": "$index",
						"index": {
							"root": "$val",
							"val": "index"
						},
						"in": {
							"root": "$val",
							"val": "data"
						}
					}
				}
			}
		]
	},
	"TableRow": {
		"root": "tr",
		"style": {
			"background": "#999"
		},
		"children": {
			"root": "$for",
			"for": "index",
			"in": {
				"root": "$val",
				"val": "data"
			},
			"do": {
				"root": "TableCell",
				"children": {
					"root": "$index",
					"index": {
						"root": "$val",
						"val": "index"
					},
					"in": {
						"root": "$val",
						"val": "data"
					}
				}
			}
		}
	},
	"TableCell": {
		"root": "td",
		"style": {
			"border": "1px solid black"
		},
		"children": {
			"root": "$val",
			"val": "children"
		}
	},
	"AnotherTable": {
		"root": "Table"
	},
	"Box": {
		"root": "div",
		"style": {
			"root": "$merge",
			"merge": {
				"root": "$val",
				"val": "style"
			},
			"with": {
				"border": "3px solid black",
				"float": "left",
				"padding": "5",
				"margin": "5"
			}
		},
		"children": [
			{
				"root": "div",
				"children": {
					"root": "$val",
					"val": "name"
				}
			},
			{
				"root": "$val",
				"val": "children"
			}
		]
	},
	"Boxes": {
		"root": "div",
		"children": [
			{
				"root": "Box",
				"name": "My Box",
				"children": [
					{
						"root": "Box",
						"name": "My Inner Box",
						"children": {
							"root": "Box",
							"name": "My Inner Inner Box"
						}
					},
					{
						"root": "Box",
						"name": "My Second Inner Box"
					}
				]
			},
			{
				"root": "div",
				"style": {
					"clear": "both"
				}
			}
		]
	},
	"Boxes2": {
		"root": "Box",
		"name": "NOT NAME",
		"children": [
			{
				"root": "Box",
				"name": {
					"root": "$val",
					"val": "name"
				}
			}
		]
	},
	"Figure": {
		"root": "div",
		"children": [
			{
				"root": "$val",
				"val": "title"
			},
			{
				"root": "br"
			},
			{
				"root": "img",
				"src": {
					"root": "$val",
					"val": "src"
				},
				"alt": {
					"root": "$val",
					"val": "title"
				},
				"title": {
					"root": "$val",
					"val": "title"
				}
			}
		]
	},
	"Container": {
		"root": "div",
		"style": {
			"width": "100%",
			"height": "600",
			"position": "relative"
		},
		"children": {
			"root": "$val",
			"val": "children"
		}
	}
}
